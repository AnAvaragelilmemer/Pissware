--pissware mobile by drmr
repeat task.wait() until game:IsLoaded()

 local lplr = game.Players.LocalPlayer 
 local name = lplr.Name 
 local dname = lplr.DisplayName 
 local workspace = game.Workspace 
 local chlplr = lplr.Character.Humanoid
 local gravity = workspace.Gravity 
 local executor = identifyexecutor() 
 local library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))(); --draggable kavo 
 local Window = library.CreateLib("pissware : MOBILE", "Synapse"); 
  
 local home = Window:NewTab("Home"); 
 local lp = Window:NewTab("LocalPlayer"); 
 local scripts = Window:NewTab("Scripts");
 --home inspired by Vision hub 
  
 local user = home:NewSection("Hello, "..lplr.DisplayName.."!"); 
 local user = home:NewSection("Version : V1"); 
  
 user:NewDropdown("info for your roblox account", "didnt i said it.", {"Name: "..name, "Display name: "..dname, "ID: "..lplr.UserId, "Account creation in days: "..lplr.AccountAge, "Country: "..game:GetService("LocalizationService"):GetCountryRegionForPlayerAsync(lplr)},  function(currentOption) 
     print(currentOption) 
 end); 
  
 user:NewDropdown("Misc", "Additional information", {"Executor: "..executor, "Language: "..lplr.LocaleId}, function(currentOption) 
     print(currentOption) 
 end) 
  
 local Section = lp:NewSection("LocalPlayer") 
  
 Section:NewSlider("Speed", "makes you more faster", 500, 16, function(v) 
     chlplr.WalkSpeed = v 
 end) 
  
 Section:NewSlider("Jump", "makes you jump alot higher", 1000, 50, function(v) 
     chlplr.JumpPower = v 
 end) 
  
 Section:NewSlider("Gravity", "defy gravity.", 1000, 196, function(v) 
     workspace.Gravity = v 
 end) 
  
 Section:NewButton("Reset speed", "Reverts your speed to 16", function() 
     chlplr.WalkSpeed = 16 
 end) 
  
 Section:NewButton("Reset Jump", "Reverts your jump to 50", function() 
     chlplr.JumpPower = 50 
 end) 
  
 Section:NewButton("Reset Gravity", "Reverts your gravity to 196", function() 
     workspace.Gravity = 196 
 end) 
  
 local Section = lp:NewSection("Misc") 
  
 Section:NewButton("Rejoin", "self explaining", function() 
game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService('Players').LocalPlayer) 
 end)
 
Section:NewButton("Leave game", "self explaining", function()
 game:Shutdown()
 end)
 
 Section:NewButton("Unlock fps", "self explaing", function()
 setfpscap(360)
end)

Section:NewButton("Infinite jump", "self explaing.", function()
game:GetService("UserInputService").JumpRequest:connect(function()
game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")		
end)
end)

Section:NewButton("Fps boost", "boosts your fps.", function()
loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/AnAvaragelilmemer/scripts/main/fpsboost"))()
end)

local Section = scripts:NewSection("Scripts")

 Section:NewButton("Keyboard", "Replicates a keyboard for PC scripts", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)
  








